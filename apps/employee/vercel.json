{
  "headers": [
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type", 
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)", 
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "apps/employee/package.json",
      "use": "@vercel/node"
    }
  ]
}