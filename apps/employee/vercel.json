{
  "headers": [
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "buildCommand": "npm run build:employee",
      "outputDirectory": "apps/employee/dist",
      "framework": "vite"
    }
  ]
}